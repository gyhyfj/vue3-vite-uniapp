    "vite": "4.0.4",
    "vite-plugin-ejs": "^1.6.4",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-stylelint": "^4.1.8",
    "vite-svg-loader": "^4.0.0",
